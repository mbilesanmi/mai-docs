    "materialize-css": "^0.98.2",
    "morgan": "^1.8.2",
    "nodemon": "^1.11.0",
    "pg": "^6.2.3",
    "pg-hstore": "^2.3.2",
    "react": "15.0.2",
    "react-dom": "15.0.2",
