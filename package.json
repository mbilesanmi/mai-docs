{
  "name": "mai-docs",
  "version": "1.0.0",
  "description": "Mai Document Management System",
  "scripts": {
    "deploy": "npm build && npm start",
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text mocha ./client/test/**/*.test.js ./server/test/**/*.test.js --require babel-register",
    "start:dev": "nodemon ./tools/appServer.js --exec babel-node",
    "db:migrate": "NODE_ENV=development sequelize db:migrate:undo:all && sequelize db:migrate",
    "build": "rm -rf dist/ && babel server -d dist/server && npm run build:app",
    "build:client": "webpack --config webpack.config.js && cat ./client/index.html > ./dist/index.html",
    "start": "node ./tools/appServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-milesanmi/mai-docs.git"
  },
  "author": "Maranatha A. Ilesanmi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andela-milesanmi/mai-docs/issues"
  },
  "homepage": "https://github.com/andela-milesanmi/mai-docs#readme",
  "dependencies": {
    "axios": "^0.16.1",
    "babel-polyfill": "6.8.0",
